\name{rep.char}
\alias{rep.char}
\title{Repeating chars}
\usage{
  rep.char(x, n, sep = "")
}
\arguments{
  \item{x}{string to repeat}

  \item{n}{integer}

  \item{sep}{separator between repeatitions}
}
\value{
  character vector
}
\description{
  Repeating a string \code{n} times and returning a
  concatenated character vector.
}

